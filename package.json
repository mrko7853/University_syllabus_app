    "build:aliases": "node scripts/create-route-aliases.mjs",
    "build:web": "VITE_DEPLOY_TARGET=web vite build && npm run build:aliases",
    "build:mobile": "VITE_DEPLOY_TARGET=mobile vite build && npm run build:aliases",
    "build": "npm run build:web",
    "cap:sync": "npm run build:mobile && npx cap sync",
    "ios:add": "npx cap add ios",
    "ios:open": "npx cap open ios",
    "ios:dev": "npm run cap:sync && npx cap open ios",
    "ios:pods:install": "cd ios/App && pod install",
    "ios:pods:reset": "cd ios/App && pod deintegrate && rm -rf Pods Podfile.lock && pod install"
